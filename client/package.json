{
  "name": "bf-react-template",
  "version": "1.0.0",
  "description": "Blue Frontend clean template for working with React",
  "main": "src/index.js",
  "proxy": "http://localhost:5000/",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.js -d --mode=development",
    "build": "npm run clean && node ./build_scripts/build.js",
    "build:prod": "npm run set:prod && npm run build -- -e production",
    "build:dev": "npm run set:dev && npm run build -- -e development",
    "snapshot": "npm run build && react-snap",
    "set": "node ./build_scripts/setEnv.js",
    "set:dev": "npm run set -- -e development",
    "set:prod": "npm run set -- -e production",
    "set:env": "npm i && npm run set -- -e example",
    "clean": "rimraf build .cache && mkdir -p build .cache",
    "pretest": "npm run eslint",
    "flow": "flow",
    "eslint": "eslint -c .eslintrc --ignore-path .gitignore ./src",
    "test": "NODE_ENV=test && jest --config ./config/jest.config.js --detectOpenHandles --passWithNoTests",
    "test:watch": "NODE_ENV=test && jest --config ./config/jest.config.js --watchAll",
    "test:debug": "node --inspect ./node_modules/jest/bin/jest.js -i --config ./config/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.com/bluefrontend/bf-react-template"
  },
  "author": "Stijn Van Hulle <stijn.vh@blue-frontend.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-decorators": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "@epegzz/sass-vars-loader": "^3.4.0",
    "@hot-loader/react-dom": "^16.8.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.6.0",
    "cpr": "^3.0.1",
    "css-loader": "^0.28.11",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "event-hooks-webpack-plugin": "^2.1.1",
    "figlet": "^1.2.1",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.94.0",
    "husky": "^0.14.3",
    "is-docker": "^1.1.0",
    "jest": "^24.3.0",
    "jest-cli": "^24.3.0",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-loader": "^2.1.6",
    "progress-bar-webpack-plugin": "^1.12.1",
    "react-hot-loader": "^4.8.0",
    "react-snap": "^1.23.0",
    "sass-loader": "^6.0.7",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@bluefrontend/bf-component-library": "^0.1.2",
    "axios": "^0.18.0",
    "decko": "^1.2.0",
    "history": "^4.7.2",
    "moment": "^2.24.0",
    "node-fetch": "^2.3.0",
    "offline-plugin": "^5.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.8.0",
    "react-json-view": "^1.19.1",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "styled-components": "^4.1.3"
  }
}
